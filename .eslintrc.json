{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "comma-dangle": 0,
        "linebreak-style": "off",
        "semi": 0,
        "eol-last": 0,
        "import/no-extraneous-dependencies": 0,
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "no-unused-expressions": 0,
        "import/no-dynamic-require": 0,
        "global-require": 0,
        "jsx-a11y/anchor-is-valid": 0,
        "arrow-body-style": 0,
        "arrow-parens": 0,
        "no-trailing-spaces": 0,
        "key-spacing": 0
    }
}